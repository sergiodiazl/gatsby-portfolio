{
  "name": "gatsby-starter-mate",
  "version": "2.5.2",
  "description": "Gatsby v2 starter to create a top notch portfolio!",
  "main": "index.js",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "start": "yarn develop",
    "serve": "yarn build && gatsby serve -p 9000",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmaSuriano/gatsby-starter-mate.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Ema Suriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmaSuriano/gatsby-starter-mate/issues"
  },
  "homepage": "https://github.com/EmaSuriano/gatsby-starter-mate#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@rebass/forms": "^4.0.6",
    "@tippy.js/react": "^3.1.1",
    "babel-plugin-styled-components": "^2.1.4",
    "contentful": "^10.3.1",
    "dotenv": "^16.3.1",
    "envfile": "^6.18.0",
    "gatsby": "^5.11.0",
    "gatsby-plugin-google-analytics": "^2.1.34",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-preload-link-crossorigin": "^1.0.2",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-styled-components": "^6.11.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-contentful": "^8.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^5.11.0",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "inquirer": "^7.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-headroom": "^3.2.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.7",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^3.2.5",
    "react-text-loop": "^2.3.0",
    "rebass": "^4.0.7",
    "styled-components": "^6.0.1"
  },
  "devDependencies": {
    "axe-core": "^4.7.2",
    "chalk": "^4.1.2 ",
    "contentful-import": "^8.5.60",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.2.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
